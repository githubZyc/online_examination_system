<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.online_examination_system.bean.TeacherBean" table="teacher">
		<id name="id" column="id" type="int">
			<generator class="increment"></generator>
		</id>
		<property name="tno" column="tno" type="string"></property>
		<property name="name" column="name" type="string"></property>
		<property name="birthday" column="birthday" type="date"></property>
		<property name="nation" column="nation" type="string"></property>
		<property name="tel" column="tel" type="string"></property>
		<property name="email" column="email" type="string"></property>
		<property name="address" column="address" type="string"></property>
		<property name="position" column="position" type="string"></property>
		<property name="password" column="password" type="string"></property>
		<property name="powerid" column="powerid" type="int"></property>
		<property name="des" column="des" type="text"></property>
	</class>
</hibernate-mapping>