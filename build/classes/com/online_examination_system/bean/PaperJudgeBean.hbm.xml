<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.online_examination_system.bean.PaperJudgeBean" table="paper_judge">
		<id name="id" column="id" type="int">
			<generator class="increment"></generator>
		</id>
		<property name="papername" column="papername" type="string"></property>
		<property name="question" column="question" type="text"></property>
		<property name="modelanswer" column="modelanswer" type="int"></property>
		<property name="teachername" column="teachername" type="string"></property>
		<property name="studentid" column="studentid" type="int"></property>
		<property name="courseid" column="courseid" type="int"></property>
		<property name="studentanswer" column="studentanswer" type="int"></property>
		<property name="score" column="score" type="int"></property>
		<property name="getscore" column="getscore" type="int"></property>
	</class>
</hibernate-mapping>