<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.online_examination_system.bean.PoliticsClassBean" table="politicsclass">
		<id name="id" column="id" type="int">
			<generator class="increment"></generator>
		</id>
		<property name="name" column="name" type="string"></property>
		<property name="majorId" column="majorid" type="int"></property>
		<property name="teacherId" column="teacherid" type="int"></property>
		<property name="des" column="des" type="text"></property>
	</class>
</hibernate-mapping>